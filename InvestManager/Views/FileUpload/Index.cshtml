@{
    ViewData["Title"] = "Realizar Upload";
}

<h2>@ViewData["Title"]</h2>

<form method="post" enctype="multipart/form-data" asp-controller="FileUpload" asp-action="Index">
    <div class="form-group row">
        <div class="col-md-2">
            <input type="file" name="archives" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-2">
            <input type="submit" class="btn btn-default btn-sm" value="Enviar Arquivo" />
        </div>
    </div>
</form>


@ViewBag.Range

