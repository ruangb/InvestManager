@model IEnumerable<InvestManager.Models.Parameter>

@{
    ViewData["Title"] = "Parâmetros";
}
<br />

<p>
    <a asp-action="Create" class="btn btn-default">Cadastrar Parâmetros</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TradingFee)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LiquidityFee)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.TradingFee)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LiquidityFee)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a>
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
